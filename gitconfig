[user]
  name = NAME
  email = EMAIL
[core]
  excludesfile = ~/.gitignore
  editor = /usr/bin/vim
[help]
  autocorrect = 1
[color]
  ui = true
[alias]
  lol = log --oneline --decorate --graph
  lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
  co = checkout -b
  com = checkout master
[push]
  default = current
[pull]
  default = current
